.withdraw_wrap {
  padding: 25rpx;
  background: #fdfdfd;
}

.withdraw_wrap .content {
  width: 700rpx;
  height: 283rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 2rpx 27rpx 0rpx rgba(56, 56, 56, 0.13);
  border-radius: 7rpx;
  padding: 0 36rpx;
  box-sizing: border-box;
  overflow: hidden;
}

.withdraw_btn {
  width: 400rpx;
  height: 68rpx;
  line-height: 68rpx;
  background: linear-gradient(18deg, rgba(87, 91, 221, 1) 0%, rgba(86, 163, 254, 1) 100%);
  box-shadow: 0rpx 6rpx 16rpx 0rpx rgba(25, 113, 214, 0.28);
  border-radius: 34rpx;
  font-size: 25rpx;
  color:#fff;
  font-weight:300;
  margin-top: 73rpx;
}


.withdraw_wrap .title{
  display: flex;
  align-items: center;
  margin-top: 32rpx;
}
.withdraw_wrap .content .icon{
  width: 34rpx;
  height: 30rpx;
  background-size: 100% 100%;
  margin-right: 17rpx;
}
.withdraw_wrap .content text{
  font-size: 36rpx;
  font-weight:300;
}
.withdraw_moeny{
  display: flex;
  align-items: center;
  font-weight:300;
  border-bottom: 1rpx solid rgba(0,0,0,0.07);
  margin-top: 50rpx;
  padding-bottom: 30rpx;
  justify-content: space-between;
}
.withdraw_moeny .money{
  font-size: 30rpx;
  padding-left: 10rpx;
}
.withdraw_wrap .withdraw_moeny .money_unit{
  font-size: 48rpx;
}
.withdraw_wrap .withdraw_moeny .all_money{
  color: #56A5FF;
  font-size: 30rpx;
}
.withdraw_moeny .withdraw_left{
  display: flex;
  align-items: center;
}

