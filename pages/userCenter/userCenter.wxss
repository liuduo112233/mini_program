.user-container{
  box-sizing: border-box;
  padding:0 25px 10px 25px;
}
.userinfo {
  display: flex;
  flex-direction: column;
  padding: 50rpx;
  align-items: center
}

.userinfo-avatar{
  width:128rpx;
  height:128rpx;
  margin:20rpx;
  border-radius:50%;
}
.userinfo-nickname{
  color:#aaa;
}
.item{
  height: 60px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid rgba(0,0,0,0.12);
}
.item-title{
  color: #333;
  font-size: 16px;
}
.item-icon{
 width: 22px;
 height: 22px;
}
.bottom_tabWrap {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 112rpx;
  border-top: 1rpx solid rgba(0, 0, 0, 0.15);
}

.bottom_tabWrap .tab_item {
  width: 50%;
  display: inline-block;
  text-align: center;
}

.bottom_tabWrap .tab_item .title {
  font-size: 24rpx;
  color: rgba(0, 0, 0, 0.3);
  display: block;
  line-height: 20rpx;
}

.bottom_tabWrap .tab_img {
  margin-top: 20rpx;
}

.bottom_tabWrap .home_img {
  width: 44rpx;
  height: 40rpx;
}

.bottom_tabWrap .user_img {
  width: 41rpx;
  height: 45rpx;
  margin-top: 25rpx;
}

.bottom_tabWrap .tab_item.active .title{
  color: #56A5FF;
}






